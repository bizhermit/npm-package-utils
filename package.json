{
  "name": "@bizhermit/npm-package-utils",
  "version": "0.0.0-rc.2",
  "description": "npm package utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/bizhermit/npm-package-utils.git"
  },
  "bugs": "https://github.com/bizhermit/npm-package-utils/issues",
  "author": "Senda Ryoichi <rsenda@bizhermit.com> (https://bizhermit.com)",
  "homepage": "https://bizhermit.com",
  "license": "MIT",
  "scripts": {
    "license": "npx rimraf CREDIT && npx license -o CREDIT --returnError",
    "build": "npm run license && npx rimraf package && npx tsc -p src/tsconfig.json && npx minifier ./package && node package/cli pack",
    "dev:init": "node package/cli init",
    "dev:pack": "node package/cli pack",
    "dev:error": "node package/cli"
  },
  "devDependencies": {
    "@bizhermit/license": "^1.0.10",
    "@bizhermit/minifier": "^1.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  }
}
